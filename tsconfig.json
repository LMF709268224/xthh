{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["dom", "es5", "es2015.promise"],
    "target": "es5",
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    //"strict": true,
    //"strictNullChecks": true,
    "noEmitOnError": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
  },
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings",
  ]
}